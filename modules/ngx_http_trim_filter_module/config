ngx_addon_name=ngx_http_trim_filter_module
HTTP_TRIM_FILTER_SRCS="$ngx_addon_dir/ngx_http_trim_filter_module.c"

if test -n "$ngx_module_link"; then
    ngx_module_type=HTTP_AUX_FILTER
    ngx_module_name=ngx_http_trim_filter_module
    ngx_module_incs=
    ngx_module_deps=
    ngx_module_srcs=$HTTP_TRIM_FILTER_SRCS
    ngx_module_libs=

    . auto/module
else
    HTTP_AUX_FILTER_MODULES="$HTTP_AUX_FILTER_MODULES ngx_http_trim_filter_module"
    NGX_ADDON_SRCS="$NGX_ADDON_SRCS $HTTP_TRIM_FILTER_SRCS"
fi
